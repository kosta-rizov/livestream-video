generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  is             String  @id @default(uuid()) @map("_id")
  username       String  @unique
  imageUrl       String
  externalUserId String  @unique
  bio            String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
